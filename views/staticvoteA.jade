                    |                                    
                    ul.ss-choices(aria-label='What is your food mood?', role='radiogroup' style="list-style:none")
                      li.ss-choice-item
                        label
                          span.ss-choice-item-control.goog-inline-block
                            input#group_645480998_1.ss-q-radio(aria-label='Hearty', name='entry.645480998', role='radio', type='radio', value='Hearty')
